<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping assembly="NHProvider.Model" namespace="nhprovider.model" xmlns="urn:nhibernate-mapping-2.2">
    <class name="AspnetApplication" table='aspnet_Applications'>
        <!-- Primary key is auto-generated by the database's native method. -->
      <id name="ApplicationId" type="Guid">
        <column name="ApplicationId" default="newid()" not-null="true" sql-type="uniqueidentifier" />
        <generator class="guid.comb" />
      </id>
      <property name="ApplicationName" type="String">
        <column name="ApplicationName" not-null="true" length="256" sql-type="nvarchar" unique="true" />
      </property>
      <property name="LoweredApplicationName" type="String">
        <column name="LoweredApplicationName" not-null="true" length="256" sql-type="nvarchar" unique="true" />
      </property>
      <property name="Description" type="String">
        <column name="Description" not-null="false" length="256" sql-type="nvarchar" />
      </property>
      <bag name="AspnetMemberships" inverse="true" generic="true">
        <key>
          <column name="ApplicationId" />
        </key>
        <one-to-many class="AspnetMembership" />
      </bag>
      <bag name="AspnetRoles" inverse="true" generic="true" lazy="false">
        <key>
          <column name="ApplicationId" />
        </key>
        <one-to-many class="AspnetRole" />
      </bag>
      <bag name='AspnetUsers' inverse='true' generic='true'>
        <key>
          <column name="ApplicationId" />
        </key>
        <one-to-many class='AspnetUser' />
      </bag>
      <!--<bag name="AspnetUsers" inverse="true" generic="false">
        <key>
          <column name="ApplicationId" />
        </key>
        <one-to-many class="AspnetUser" />
      </bag>-->
    </class>
</hibernate-mapping>